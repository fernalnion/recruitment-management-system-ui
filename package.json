{
  "name": "rms-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:rms-ui": "node dist/rms-ui/server/server.mjs",
    "build:install": "npm install -D  --production=false",
    "lint": "ng lint",
    "build:prod": "ng build --configuration production --aot --output-hashing=all --source-map=false --named-chunks=false --extract-licenses=true --optimization=true"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.0.4",
    "@angular/common": "17.0.4",
    "@angular/compiler": "17.0.4",
    "@angular/core": "17.0.4",
    "@angular/forms": "17.0.4",
    "@angular/platform-browser": "17.0.4",
    "@angular/platform-browser-dynamic": "17.0.4",
    "@angular/platform-server": "17.0.4",
    "@angular/router": "17.0.4",
    "@angular/ssr": "17.0.2",
    "express": "4.18.2",
    "rxjs": "~7.8.1",
    "tslib": "2.6.2",
    "zone.js": "~0.14.2",
    "eslint": "8.54.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.0.2",
    "@angular-eslint/schematics": "17.1.0",
    "@angular/cli": "17.0.2",
    "@angular/compiler-cli": "17.0.4",
    "@types/express": "4.17.21",
    "@types/jasmine": "~5.1.3",
    "@types/node": "20.9.3",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "5.2"
  }
}